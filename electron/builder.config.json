{
  "appId": "com.techindeck.launcher",
  "productName": "AngularElectron",
  "files": ["web/**/*", "app/**/*", "splash.html"],
  "copyright": "Copyright Â© 2022 ${author}",
  "removePackageScripts": true,
  "generateUpdatesFilesForAllChannels": true,
  "compression": "normal",
  "win": {
    "target": [
      {
        "target": "nsis-web",
        "arch": ["x64", "ia32"]
      },
      "zip"
    ],
    "icon": "app/icon.ico",
    "publisherName": "OpEzee"
  },
  "mac": {
    "category": "your.app.category.type",
    "target": "dmg"
  }
}
